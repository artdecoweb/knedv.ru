{
  "name": "@artdeco/knedv.ru",
  "version": "0.0.0",
  "description": "The Realty Corporation 21 Century Website.",
  "main": "build",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "start": "NODE_ENV=production node src",
    "dev": "node src",
    "emulate": "NODE_ENV=emulate-prod node src",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "d": "NODE_DEBUG=doc doc src/server.js -g",
    "build": "alamode src -o build -s -i bin/index.js",
    "depack": "depack-dev frontend-admin/index.jsx -o build/admin.js -I 2018 -a -H --charset=UTF-8 -p --externs frontend-admin/externs/index.js -w",
    "dokku": "git push dokku"
  },
  "files": [
    "build"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/artdecoweb/knedv.ru.git"
  },
  "keywords": [
    "knedv.ru",
    "artdeco"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/artdecoweb/knedv.ru/issues"
  },
  "homepage": "https://github.com/artdecoweb/knedv.ru#readme",
  "devDependencies": {
    "alamode": "1.8.4",
    "documentary": "1.22.0",
    "eslint-config-artdeco": "1.0.1",
    "mongodb": "3.1.13",
    "parse-multipart": "1.0.4",
    "rqt": "3.1.2",
    "zoroaster": "3.8.2"
  },
  "dependencies": {
    "@demimonde/dotenv": "1.0.0",
    "@depack/form": "1.2.1",
    "@depack/render": "1.1.4",
    "@depack/router": "1.0.3",
    "@idio/core": "1.3.5",
    "@idio/mailru": "1.0.0",
    "@idio/router": "1.2.0",
    "@wrote/rm": "1.1.2",
    "azure-storage": "2.10.2",
    "dist-exiftool": "10.53.0",
    "erte": "1.1.6",
    "logarithm": "1.0.1",
    "mongoose": "5.4.19",
    "node-exiftool": "2.3.0",
    "preact": "8.4.2",
    "sharp": "0.21.3",
    "shortid": "2.2.14",
    "trumbowyg": "2.14.0",
    "unfetch": "4.1.0"
  }
}
